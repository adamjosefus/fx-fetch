{
  "name": "fx-fetch",
  "version": "0.1.2",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "zshy --project tsconfig.build.json",
    "circular": "dpdm --exit-code circular:1 ./src/index.ts",
    "docgen": "typedoc --options ./typedoc.json",
    "fmt": "biome check . --error-on-warnings",
    "fmt:write": "biome check . --write --error-on-warnings",
    "prepare": "pnpm run /prepare:/",
    "prepare:effect-language-service": "effect-language-service patch",
    "prepare:husky": "husky",
    "prepublishOnly": "pnpm run validate && pnpm run test && pnpm run fmt && pnpm run build",
    "test": "vitest run ./src",
    "test:watch": "vitest watch ./src",
    "validate": "tsc"
  },
  "dependencies": {
    "effect": "^3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@effect/language-service": "0.62.1",
    "@types/node": "^20",
    "dpdm": "^3.14.0",
    "husky": "^9.1.7",
    "typedoc": "^0.28.15",
    "typedoc-github-theme": "^0.3.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15",
    "zshy": "^0.6.0"
  },
  "peerDependencies": {
    "effect": "^3"
  },
  "zshy": {
    "exports": "./src/index.ts",
    "cjs": false,
    "esm": true
  }
}
