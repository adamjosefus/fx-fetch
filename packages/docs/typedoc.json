{
  "entryPoints": ["../fx-fetch/src/index.ts"],
  "packageOptions": {
    "includeVersion": true
  },
  "name": "FxFetch",
  "favicon": "./public/favicon.svg",
  "projectDocuments": [
    "./contents/00-introduction.md",
    "./contents/01-request.md",
    "./contents/02-fetch.md",
    "./contents/03-response.md",
    "./contents/04-url.md"
  ],
  "headings": {
    "readme": false,
    "document": false
  },
  "customCss": "./overwrite.css",
  "groupOrder": ["Docs", "*"],
  "outputs": [
    {
      "name": "html",
      "options": {
        "navigation": {
          "excludeReferences": true,
          "includeCategories": true,
          "includeFolders": true,
          "includeGroups": true
        }
      },
      "path": "./dist"
    }
  ],
  "plugin": ["typedoc-github-theme"],
  "tsconfig": "../fx-fetch/tsconfig.json"
}
