{
  "name": "fx-fetch",
  "version": "1.0.4",
  "description": "Simple, immutable, clonable, and effect-based HTTP fetching.",
  "homepage": "https://adamjosefus.github.io/fx-fetch/",
  "repository": {
    "type": "git",
    "url": "https://github.com/adamjosefus/fx-fetch.git",
    "directory": "packages/fx-fetch"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./Url": {
      "types": "./dist/Url/index.d.ts",
      "default": "./dist/Url/index.js"
    },
    "./Request": {
      "types": "./dist/Request/index.d.ts",
      "default": "./dist/Request/index.js"
    },
    "./Response": {
      "types": "./dist/Response/index.d.ts",
      "default": "./dist/Response/index.js"
    },
    "./Cause": {
      "types": "./dist/Cause/index.d.ts",
      "default": "./dist/Cause/index.js"
    },
    "./Fetch": {
      "types": "./dist/Fetch/index.d.ts",
      "default": "./dist/Fetch/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "zshy --project tsconfig.build.json",
    "circular": "dpdm --exit-code circular:1 ./src/index.ts",
    "fmt": "biome check . --error-on-warnings",
    "fmt:write": "biome check . --write --error-on-warnings",
    "prepare": "effect-language-service patch",
    "prepublishOnly": "pnpm run validate && pnpm run test && pnpm run fmt && pnpm run build",
    "test": "vitest run ./src",
    "validate": "tsc"
  },
  "dependencies": {
    "@types/node": ">=20",
    "effect": "^3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@effect/language-service": "0.64.1",
    "@types/node": "^24",
    "dpdm": "^3.14.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "zshy": "^0.7.0"
  },
  "zshy": {
    "exports": {
      ".": "./src/index.ts",
      "./Url": "./src/Url/index.ts",
      "./Request": "./src/Request/index.ts",
      "./Response": "./src/Response/index.ts",
      "./Cause": "./src/Cause/index.ts",
      "./Fetch": "./src/Fetch/index.ts"
    },
    "cjs": false,
    "esm": true
  }
}
